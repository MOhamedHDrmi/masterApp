						<param name="tableName" value="'X_ENTITY_LIST1'" />
						<param name="dialogScreenId" value="'X_ENTITY_LIST1'" />
						<param name="rowTypeDescription" value="GetProperty('rowTypeDescription.CASE.X_ENTITY_LIST1.txt')" />
